<div class="parent-id">
  <div><img src="{{image}}" alt="{{title}}" /></div>
  <div class="text-id">
    <h1>{{title}}</h1>
    <h2>Genre: {{genre}}</h2>
    <h2><strong>cast</strong></h2>
    <ul>
      {{#each cast}}
        <li>{{this}}</li>
      {{/each}}
    </ul>
    <br />
    <p>{{plot}}</p>

  </div>
</div>